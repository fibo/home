# Git
alias ga='git add .'
alias gc='git commit'
alias gd='git diff'
alias gl='git log --graph'
alias gpl='git pull'
alias gps='git push'
alias gpsf='git push --force-with-lease'
alias gs='git status'

# npm
alias ns='npm start'
alias nt='npm test'

# Go to up folder
alias ',,'='cd .. && pwd'
" List files and folders
alias ','='ls -Galrth'
